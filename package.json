{
	"version": "1.0.0",
	"license": "MIT",
	"main": "lib/index.js",
	"module": "es/index.js",
	"typings": "lib/index.d.ts",
	"unpkg": "dist/axhr-umd.min.js",
	"name": "axhr",
	"author": "NARUTOne",
	"keywords": [
		"axhr",
		"axios",
		"xhr",
		"request"
	],
	"files": [
		"dist",
		"es",
		"lib"
	],
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=9.0.0"
	},
	"scripts": {
		"start": "dumi dev",
		"clean": "rimraf dist es lib",
		"build": "pnpm build:cjs && pnpm build:esm && pnpm build:umd",
		"build:esm": "tsc --project tsconfig.build.json --module esnext --target es2015 --outDir ./es",
		"build:cjs": "tsc --project tsconfig.build.json --module commonjs --target es5 --outDir ./lib",
		"build:umd": "rollup -c",
		"build-doc": "dumi build && node ./scripts/zip.js",
		"lint": "pnpm run lint-es",
		"lint-es": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx ./src",
		"prettier": "prettier --write **/*.{js,ts,jsx,tsx,md,json}",
		"prebuild": "pnpm run lint && pnpm run clean",
		"precommit": "pnpm lint-staged",
		"prepare": "is-ci || husky configs/husky-config",
		"release": "pnpm build && npm publish --access public",
		"test": "jest"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		],
		"rules": {
			"type-enum": [
				2,
				"always",
				[
					"feat",
					"fix",
					"style",
					"build",
					"perf",
					"test",
					"docs",
					"refactor",
					"revert",
					"chore"
				]
			],
			"body-max-line-length": [
				2,
				"always",
				300
			],
			"footer-max-line-length": [
				2,
				"always",
				300
			]
		}
	},
	"lint-staged": {
		"**/*.{js,jsx,tsx,ts,less,json}": [
			"pnpm prettier"
		]
	},
	"devDependencies": {
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@rollup/plugin-commonjs": "^22.0.2",
		"@rollup/plugin-node-resolve": "^14.0.1",
		"@rollup/plugin-replace": "^4.0.0",
		"@rollup/plugin-typescript": "^11.1.6",
		"@types/jasmine-ajax": "^3.3.5",
		"@types/jest": "^29.0.0",
		"@typescript-eslint/eslint-plugin": "^7.1.1",
		"@typescript-eslint/parser": "^7.1.1",
		"devmoji": "^2.3.0",
		"dumi": "^2.4.13",
		"eslint": "^8.23.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^9.1.7",
		"is-ci": "^3.0.1",
		"jasmine-ajax": "^4.0.0",
		"jasmine-core": "^5.6.0",
		"jest": "^29.0.2",
		"jszip": "^3.10.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"rollup": "^2.79.0",
		"rollup-plugin-filesize": "^9.1.2",
		"rollup-plugin-terser": "^7.0.2",
		"ts-jest": "^29.1.2",
		"typescript": "^5.3.3"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all",
			"not ie < 11"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"description": "axhr axios",
	"dependencies": {
		"axios": "^1.8.4",
		"lodash": "^4.17.21"
	}
}